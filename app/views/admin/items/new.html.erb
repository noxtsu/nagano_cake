<h2>商品新規登録</h2>

<%= form_with model: @items, url: new_admin_item_path, local:true do |f| %>

　<h4>商品画像</h4>
  <%= f.attachment_field :image %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明</h4>
  <%= f.text_field :introduction %>
  <h4>ジャンル</h4>
  
  <h4>販売価格</h4>
  <%= f.text_field :price %>円
  
  <h4>販売ステータス</h4>
  
  
  <%= f.submit '新規登録' %>
  
  

<% end %>